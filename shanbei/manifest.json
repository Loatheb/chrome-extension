{
  "manifest_version": 2,

  "name": "shanbei",
  "description": "created by zhangzhao",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": ["tabs", "https://api.shanbay.com/*"],
  "content_scripts": [{
    "matches": ["https://www.theguardian.com/*", "http://www.theguardian.com/*"],
    "css": ["custom.css"],
    "js": [ "funs.js", "dialog.js" ,"translate.js" ,
             "page.js", "removeAd.js" , "index.js"]
}]
}
